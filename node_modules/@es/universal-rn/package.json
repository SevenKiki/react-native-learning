{
  "name": "@es/universal-rn",
  "version": "0.0.8",
  "description": "",
  "main": "./src/index.ts",
  "react-native": "./index.js",
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:lib": "bash ../../build/build.tile.sh ./packages/universal-rn",
    "build:es": "tsc --module es6 --outDir ./",
    "build:cjs": "tsc --module commonjs --outDir ./cjs",
    "start": "tsc --watch --module es6 --outDir ./",
    "git:commit": "git add . && git commit -m \"deploy: commit for build\" && npm run git:push",
    "git:push": "git push origin master",
    "check": "es-check es5 --module=es ./*.js ./**/*.js",
    "deploy": "npm run build && npm run check  && npm version patch && npm run git:commit && npm publish",
    "clean": "bash ../../build/clean.lib.sh",
    "clean:build": "bash ../../build/clean.lib.sh delbuildts"
  },
  "author": "yangyang06",
  "license": "ISC",
  "peerDependencies": {
    "react-native": "0.62.2"
  },
  "devDependencies": {
    "@types/react-native": "^0.65.9"
  }
}
