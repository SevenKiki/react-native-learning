{
    "name": "@yoda/bridge",
    "version": "2.0.7",
    "description": "SDK for invoking native bridges in Yoda container",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./es/yoda.mjs",
            "require": "./lib/yoda.js"
        }
    },
    "main": "lib/yoda.js",
    "module": "es/yoda.js",
    "unpkg": "dist/yoda.min.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "es",
        "lib",
        "src",
        "types"
    ],
    "scripts": {
        "prepare": "husky install",
        "clean": "rimraf dist es lib types",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "test": "jest",
        "start": "rollup --config rollup.config.ts --configPlugin typescript --watch",
        "build": "rollup --config rollup.config.ts --configPlugin typescript && tsc --project tsconfig.types.json",
        "release": "ts-node scripts/release.ts",
        "upload": "ts-node scripts/upload.ts"
    },
    "author": "yanshi (yanshi@kuaishou.com)",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://git.corp.kuaishou.com/ks-frontend/yoda/bridge.git"
    },
    "keywords": [
        "yoda",
        "bridge"
    ],
    "typeCoverage": {
        "cache": true,
        "strict": true
    },
    "dependencies": {
        "@babel/runtime": "^7.20.0",
        "@babel/runtime-corejs3": "^7.20.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.13",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-replace": "^4.0.0",
        "@rollup/plugin-typescript": "^8.4.0",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.11.8",
        "@types/semver": "^7.3.13",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "@yoda/bridge-types": "^2.0.0",
        "core-js": "^3.25.0",
        "enquirer": "^2.3.6",
        "eslint": "^8.23.0",
        "execa": "^5.1.1",
        "husky": "^8.0.1",
        "jest": "^28.1.2",
        "lint-staged": "^13.0.3",
        "rimraf": "^3.0.2",
        "rollup": "^2.78.1",
        "rollup-plugin-terser": "^7.0.2",
        "semver": "^7.3.7",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.0",
        "type-coverage": "^2.22.0",
        "typescript": "^4.8.2"
    }
}
