    "scripts": {
        "dev": "rm -rf dist && cross-env NODE_ENV=development rollup -c -w",
        "dev:plugins": "rm -rf dist && cross-env NODE_ENV=plugins rollup -c -w",
        "dev:browser": "rm -rf lib && cross-env NODE_ENV=browser node --max-old-space-size=6000 node_modules/.bin/rollup -c rollup.lib.config.js -w",
        "dev:krn": "rm -rf dist && cross-env NODE_ENV=krn rollup -c rollup.lib.config.js -w",
        "dev:mina": "rm -rf dist && cross-env NODE_ENV=mina rollup -c rollup.lib.config.js -w",
        "demo": "npm run dev & sh shell/demo.sh",
        "demo:old": "node ./demo/browser/server.js",
        "demo:krn": "npm run dev:krn & node ./demo/krn/dev-script.js",
        "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
        "docs:dev": "cp CHANGELOG.md docs/changelog.md && vuepress dev docs",
        "test": "npm run test:api && npm run test:feature",
        "docs:build": "cp CHANGELOG.md docs/changelog.md && vuepress build docs",
        "test:api": "node --inspect node_modules/.bin/jest ./test/platform  --runInBand",
        "test:feature": "node --inspect node_modules/.bin/jest ./test/feature --runInBand",
        "test:plugins": "node --inspect node_modules/.bin/jest ./test/plugins --runInBand",
        "test:coverage": "npm run test:api -- --coverage && npm run test:feature -- --coverage && run test:plugins -- --coverage",
        "build": "rm -rf dist && rm -rf lib && cross-env PUBLISH=true npm run build:main && cross-env PUBLISH=true npm run build:lib",
        "build:main": "node --max-old-space-size=8192 node_modules/.bin/rollup -c",
        "build:lib": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.lib.config.js",
        "pub:cdn": "node cdn.js",
        "kim": "node kim.js",
        "prepublishOnly": "npm run test:api && npm run build && npm run test:feature",
        "postpublish": "npm run pub:cdn && git push && git push --tags && npm run kim",
        "pub:beta": "npm version prerelease --preid=beta && npm publish --tag beta",
        "pub:rc": "npm version prerelease --preid=rc && npm publish --tag rc",
        "pub:radar": "npm version prerelease --preid=radar && npm publish --tag radar",
        "pub": "npm version patch && npm publish",
        "build:analyzer": "rm -rf bundleSizeVisualizer && cross-env PUBLISH=true CREATE_ANALYZE=true npm run build && node ./showBundleSize.js",
        "build:lib:analyzer": "rm -rf bundleSizeVisualizer && cross-env PUBLISH=true NODE_ENV=browser CREATE_ANALYZE=true npm run build:lib && node ./showBundleSize.js",
        "deploy-docs-staging": "kfx deploy staging --appid=4896a114c8 --pid=4472",
        "deploy-docs-prod": "kfx deploy prod --appid=4896a114c8 --pid=4472"
    },
    "repository": {
