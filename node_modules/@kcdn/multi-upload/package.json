        "test:watch": "NODE_ENV=test mocha --recursive --no-exit --watch",
        "test:coverage": "NODE_ENV=test nyc --reporter=text npm run test",
        "test:bin": "NODE_ENV=test node ./bin/test-bin.js",
        "test:bin-clear": "NODE_ENV=test node ./bin/test-bin.js --no-console",
        "test:propmt": "NODE_ENV=test node ./upload.js --pid=test-dzr002"
