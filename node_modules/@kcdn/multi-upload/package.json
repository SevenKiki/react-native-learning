        "dev:error": "NODE_ENV=test node ./upload.js debug-files/test-files/error --exit-on-api-error=unknown --cdn-dir='/npm-kcdn-upload__normal/' --pid=test-dzr002 --token=12228_61c3981f3a04d0a97cf39f9c20820e94 --allow-rewrite=true --allow-hash=false",
        "dev:pipeline": "NODE_ENV=test CI_JOB_ID=12345 node ./upload.js debug-files/dev-files --exit-on-api-error=unknown --cdn-dir='/npm-kcdn-upload__normal/' --pid=test-dzr002 --token=12228_61c3981f3a04d0a97cf39f9c20820e94 --allow-rewrite=true --allow-hash=false",
        "dev:debug": "NODE_ENV=test DEBUG=cdn-upload node ./upload.js debug-files/dev-files --exit-on-api-error=unknown --cdn-dir='/npm-kcdn-upload__normal/' --pid=test-dzr002 --token=12228_61c3981f3a04d0a97cf39f9c20820e94 --allow-rewrite=false --allow-hash=false",
        "debug": "NODE_ENV=test DEBUG=cdn-upload* node ./upload.js debug-files/dev-files --exit-on-api-error=unknown --cdn-dir='/npm-kcdn-upload__normal/' --pid=test-dzr002 --token=12228_61c3981f3a04d0a97cf39f9c20820e94 --allow-rewrite=true --allow-hash=false",
