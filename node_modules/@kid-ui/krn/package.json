  "scripts": {
    "start": "npm-run-all --parallel start:rn start:web",
    "start:rn": "krn start -r",
    "start:web": "webpack serve --inline --config ./buildweb/webpack.config.dev.js --env mode=development ",
    "prepublishOnly": "node ./scripts/prepublish.js && npm run build:tsc",
    "build:tsc": "tsc && node ./handStyleFile.js",
    "preinstall": "node ./scripts/preinstall.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:webpack": "vue-cli-service build ./example/index.js --no-module",
    "build:ios": "vue-cli-service build ./example/index.js --no-module --platform=ios --dest ./dist/ios",
    "build:android": "vue-cli-service build ./example/index.js --no-module --platform=android --dest ./dist/android",
    "build:web-test": "webpack  --config ./buildweb/webpack.config.pro.js --env mode=test",
    "build:web": "webpack  --config ./buildweb/webpack.config.pro.js --env mode=production",
    "lint": "npm run lint:js  && npm run lint:format",
    "lint:js": "eslint src --fix --ext .js,.jsx,.ts,.tsx --cache --cache-location node_modules/.cache/eslint/ --color",
    "lint:format": "prettier --write  \"src/**/*.{js,json,ts,tsx,css,less,scss,html,md}\"",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js"
  },
